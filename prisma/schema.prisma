generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model ContactForm {
  id      Int    @id @default(autoincrement())
  name    String @db.VarChar(100)
  email   String @unique @db.VarChar(255)
  subject String @db.VarChar(300)
  message String
}
